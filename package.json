{
  "name": "developers-today",
  "version": "0.0.6",
  "author": "pistis",
  "description": "A task management program that records and summarizes a developer's daily work.",
  "homepage": ".",
  "main": "electron/main.js",
  "private": true,
  "dependencies": {
    "@coreui/chartjs": "^2.0.0",
    "@coreui/coreui": "^3.4.0",
    "@coreui/icons": "^1.0.1",
    "@coreui/icons-react": "^1.0.2",
    "@coreui/react": "^3.4.5",
    "@coreui/react-chartjs": "^1.0.1",
    "@coreui/utils": "^1.3.1",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/jest": "26.0.15",
    "chart.js": "^2.9.4",
    "classnames": "^2.2.6",
    "core-js": "^3.8.3",
    "electron-devtools-installer": "^3.1.1",
    "electron-is-dev": "^1.2.0",
    "electron-reload": "^1.5.0",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "query-string": "^6.13.8",
    "react": "^17.0.1",
    "react-chartjs-2": "^2.11.1",
    "react-date-range": "^1.1.3",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "redux": "^4.0.5",
    "sequelize": "^6.4.0",
    "sequelize-cli": "^6.2.0",
    "sqlite3": "^5.0.1",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "postinstall": "electron-builder install-app-deps",
    "electron:dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron:windev": "concurrently \"SET BROWSER=none && yarn start\" \"wait-on http://localhost:3000 && tsc -p electron -w\" \"wait-on http://localhost:3000 && tsc -p electron && electron .\"",
    "electron:build": "yarn build && electron-builder",
    "electron:build:mac": "yarn build && electron-builder build --mac",
    "eject": "react-scripts eject"
  },
  "build": {
    "asar": false,
    "appId": "com.pistis.developers-today",
    "productName": "Developers Today",
    "copyright": "Copyright Â© 2021 pistis",
    "buildVersion": "0.0.6-beta",
    "protocols": {
      "name": "devtoday",
      "schemes": [
        "devtoday"
      ]
    },
    "mac": {
      "target": [
        "default"
      ],
      "icon": "./install/today.icns"
    },
    "dmg": {
      "title": "Developers Today",
      "icon": "./install/today.icns"
    },
    "extends": null,
    "files": [
      "build/**/*",
      "electron/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/chart.js": "^2.9.30",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.22",
    "@types/react": "^17.0.0",
    "@types/react-calendar": "^3.1.2",
    "@types/react-date-range": "^1.1.3",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "concurrently": "^5.3.0",
    "electron": "^11.2.1",
    "electron-builder": "^22.9.1",
    "node-sass": "^4.14.1",
    "wait-on": "^5.2.1"
  }
}
